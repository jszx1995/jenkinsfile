#!groovy

String buildshell = "${env.buildshell}"

pipeline {
    agent { node { label "master"}}
    
    stages {
        stage("build"){
            steps{
                script{
                    mvnhome = tool "M2"
                    sh "${mvnhome}/bin/mvn -v}"
                    println(${buildshell})
                }
            }
        }
    }
}
